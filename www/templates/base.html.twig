<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
        <link rel="stylesheet" href="/assets/css/style.css">
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
    <div class="overlay"></div>
    <header id="header-main" class="align-items-center layout-is-open">
        <div class="header-logo">
            <a href="/">
                <img class="img-fluid logo_big" src="/assets/img/logo.png">
                <img class="img-fluid logo_small" src="/assets/img/logo_small.png">
            </a>
        </div>
        <div class="header-search">
            <form action="/search" class="site-search-form">
                <div class="input-group">
                    <div id="search-wait" class="progress search-wait" style="display: none">
                        <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar"
                             aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%"></div>
                    </div>
                    <div data-toggle="tooltip" data-placement="bottom" title="Поиск без API поставщиков"
                         class="input-group-prepend" style="margin-right: -5px;">
                        <div class="input-group-text">
                            <input name="API" value="N" type="checkbox" aria-label="Поиск без API поставщиков">
                        </div>
                    </div>
                    <input value="{{app.request.get('q')}}" type="text" name="q" class="form-control main-search-inp" placeholder="Код или наименование запчасти, например: 45873">
                    <div class="input-group-append">
                        <button class="btn btn-outline-secondary align-items-center search-btn waves-effect" type="submit">
                            <svg viewBox="0 0 24 24" preserveAspectRatio="xMidYMid meet" focusable="false" class="style-scope d-flex" style="pointer-events: none; width: 20px; height: 20px;">
                                <g class="style-scope">
                                    <path d="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z" class="style-scope yt-icon"></path>
                                </g>
                            </svg>
                        </button>
                    </div>
                </div>
            </form>
        </div>
        <div class="header-phones text-right">
            <a href="tel:+79230530362" class="header-phones__phone">
                <span class="phone_top_large">
                    +7 (923) 053-03-62
                </span>
                <span class="phone_top_small">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path fill="none" d="M0 0h24v24H0V0z"/><path d="M20.1 7.7l-1 1c1.8 1.8 1.8 4.6 0 6.5l1 1c2.5-2.3 2.5-6.1 0-8.5zM18 9.8l-1 1c.5.7.5 1.6 0 2.3l1 1c1.2-1.2 1.2-3 0-4.3zM14 1H4c-1.1 0-2 .9-2 2v18c0 1.1.9 2 2 2h10c1.1 0 2-.9 2-2V3c0-1.1-.9-2-2-2zm0 19H4V4h10v16z"/></svg>
                </span>
            </a>
        </div>
        <button onclick="controlAsside()" class="header-open-button md-button layout-is-open" type="button" title="open" aria-label="open search and filters">
            <svg xmlns="http://www.w3.org/2000/svg" width="24px" height="24px" viewBox="0 0 24 24">
                <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z"></path>
            </svg>
        </button>
    </header>
    <main id="main" class="layout-is-open" >
            {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                <a href="{{ path('fos_user_security_logout') }}">
                    {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                </a>
            {% else %}
                <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
            {% endif %}
        {% block body %}{% endblock %}
